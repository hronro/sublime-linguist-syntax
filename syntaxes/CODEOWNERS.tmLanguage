							<key>name</key>
							<string>keyword.operator.glob.wildcard.globstar.codeowners</string>
							<key>match</key>
							<string>\*\*</string>
						</dict>
						<dict>
							<key>name</key>
							<string>keyword.operator.glob.wildcard.codeowners</string>
							<key>match</key>
