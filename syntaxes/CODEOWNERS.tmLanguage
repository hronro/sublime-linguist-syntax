		<key>pattern</key>
		<dict>
			<key>name</key>
			<string>meta.pattern.codeowners</string>
			<key>begin</key>
			<string>^((?:[^#\s\\]|\\[^#])++)</string>
			<key>end</key>
			<string>$|(?=#)</string>
			<key>patterns</key>
			<array>
				<dict>
