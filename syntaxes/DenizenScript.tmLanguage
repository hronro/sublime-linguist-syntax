			<key>begin</key>
			<string>(?&lt;=\s)&quot;</string>
			<key>end</key>
			<string>(?:&quot;|\n)</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>include</key>
					<string>#tags</string>
