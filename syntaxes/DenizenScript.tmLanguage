		<key>single_quotes</key>
		<dict>
			<key>name</key>
			<string>string.quoted.single.denizenscript</string>
			<key>begin</key>
			<string>(?&lt;=\s)&apos;</string>
			<key>end</key>
			<string>(?:&apos;|\n)</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>include</key>
					<string>#tags</string>
				</dict>
				<dict>
					<key>include</key>
					<string>#def_brackets</string>
				</dict>
			</array>
		</dict>
		<key>tags</key>
		<dict>
			<key>name</key>
			<string>constant.language.tag.denizenscript</string>
			<key>begin</key>
			<string>&lt;(?!-|\s|=)</string>
			<key>end</key>
			<string>&gt;</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>include</key>
					<string>#def_brackets</string>
				</dict>
				<dict>
					<key>include</key>
					<string>#tags</string>
				</dict>
			</array>
		</dict>
		<key>todo_comments</key>
		<dict>
			<key>name</key>
			<string>variable.todo-comment.denizenscript</string>
			<key>begin</key>
			<string>(?i)^\s*#\s*(?:todo)</string>
			<key>end</key>
			<string>\n</string>
		</dict>
