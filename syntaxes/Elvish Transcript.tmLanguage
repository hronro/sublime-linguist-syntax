<plist version="1.0">
<dict>
	<key>name</key>
	<string>Elvish transcript</string>
	<key>scopeName</key>
	<string>source.elvish-transcript</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>contentName</key>
			<string>meta.embedded.block.elvish</string>
			<key>begin</key>
			<string>(^|\G)[~/][^ ]*&gt; </string>
			<key>while</key>
			<string>(^|\G)   </string>
			<key>patterns</key>
			<array>
				<dict>
					<key>include</key>
					<string>source.elvish</string>
				</dict>
			</array>
		</dict>
	</array>
</dict>
</plist>
