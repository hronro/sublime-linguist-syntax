		<key>constants_language</key>
		<dict>
			<key>name</key>
			<string>constant.language.jolie</string>
			<key>match</key>
			<string>\b(true|false)\b</string>
		</dict>
		<key>constants_numeric</key>
		<dict>
			<key>name</key>
			<string>constant.numeric.jolie</string>
			<key>match</key>
			<string>\b\d+\b</string>
		</dict>
		<key>definitions</key>
		<dict>
			<key>match</key>
			<string>\b(inputPort|outputPort|interface|type|define|service)\s+(\w+)\b</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>keyword.other.jolie</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>meta.class.identifier.jolie</string>
				</dict>
			</dict>
		</dict>
		<key>invocations</key>
		<dict>
			<key>match</key>
			<string>\b(\w+)\s*(@)\s*(\w+)\b</string>
