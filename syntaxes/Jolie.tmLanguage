			<key>name</key>
			<string>keyword.operator.jolie</string>
			<key>match</key>
			<string>\b(&lt;&lt;|&amp;&amp;|\|\||\+|\-|/|\*|=|==|\+\+|--|\+=|-=|\*=|/=|!|%|%=)\b</string>
		</dict>
		<key>strings</key>
		<dict>
			<key>name</key>
			<string>string.quoted.double.jolie</string>
			<key>begin</key>
			<string>&quot;</string>
			<key>end</key>
			<string>&quot;</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>constant.character.escape.jolie</string>
					<key>match</key>
					<string>\\.</string>
				</dict>
			</array>
		</dict>
	</dict>
	<key>fileTypes</key>
	<array>
