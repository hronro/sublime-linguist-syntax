		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>comment.line.lolcode</string>
					<key>match</key>
					<string>(?&lt;!\S)BTW(?:[^\n]*)</string>
				</dict>
				<dict>
					<key>name</key>
					<string>comment.block.lolcode</string>
					<key>begin</key>
					<string>(?&lt;!\S)OBTW(?!\S)</string>
					<key>end</key>
					<string>(?&lt;!\S)TLDR(?!\S)</string>
