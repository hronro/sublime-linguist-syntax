			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>variable.other.lolcode</string>
					<key>match</key>
					<string>(?&lt;=\b)([A-Za-z][A-Za-z0-9_]*)\b</string>
				</dict>
				<dict>
					<key>name</key>
					<string>variable.other.lolcode</string>
					<key>match</key>
					<string>(?&lt;=CAN HAS )(\w*)(?=\?)</string>
				</dict>
			</array>
