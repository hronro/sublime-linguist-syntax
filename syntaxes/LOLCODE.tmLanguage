			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>punctuation.parenthesis.lolcode</string>
					<key>match</key>
					<string>\(|\)</string>
				</dict>
			</array>
		</dict>
		<key>strings</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>(&quot;(?:\\.|[^&quot;\\])*&quot;)|(&apos;(?:\\.|[^&apos;\\])*&apos;)</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>string.quoted.double.lolcode</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>string.quoted.single.lolcode</string>
						</dict>
					</dict>
				</dict>
			</array>
		</dict>
