        | \[ \g&lt;square&gt;*+ \]                             # Nested brackets
      )*+
      \]
      (
        (                                                   # Reference Link
          [ ]?                                              # Optional space
          \[[^\]]*+\]                                    # Ref name
        )
        |
        (                                                   # Inline Link
          \(                                               # Opening paren
          [ \t]*+                                          # Optional whitespace
          &lt;?(.*?)&gt;?                                         # URL
          [ \t]*+                                          # Optional whitespace
          (                                                 # Optional Title
            (?&lt;title&gt;[&apos;&quot;])
            (.*?)
            \k&lt;title&gt;
          )?
          \)
        )
      )
    )
    | (?!(?&lt;=\S)\1).                                      # Everything besides
  )++
