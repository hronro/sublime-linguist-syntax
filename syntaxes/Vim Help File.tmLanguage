					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.bracket.square.begin.vim-help</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.bracket.square.end.vim-help</string>
						</dict>
					</dict>
				</dict>
				<dict>
					<key>name</key>
					<string>entity.name.keyword.vim-help</string>
					<key>match</key>
					<string>\bCTRL(-)(?:.|Break|Del|Insert|PageDown|PageUp|({)char(}))</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>punctuation.delimiter.separator.dash.hyphen.vim-help</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.bracket.curly.begin.vim-help</string>
						</dict>
						<key>3</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.bracket.curly.end.vim-help</string>
						</dict>
					</dict>
				</dict>
			</array>
		</dict>
		<key>tag</key>
		<dict>
			<key>name</key>
			<string>storage.link.hypertext.vim-help</string>
			<key>match</key>
			<string>(\*)[#-)!+-~]+(\*)(?=\s|$)</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
