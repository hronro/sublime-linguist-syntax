		<key>url</key>
		<dict>
			<key>name</key>
			<string>constant.other.reference.link.vim-help</string>
			<key>match</key>
			<string>(?x)
(?:(?:(?:https?|ftp|gopher)://|(?:mailto|file|news):)[^&apos;\x20\t&lt;&gt;&quot;]+
|(?:www|web|w3)[a-z0-9_-]*\.[a-z0-9._-]+\.[^&apos;\x20\t&lt;&gt;&quot;]+)
[a-zA-Z0-9/]</string>
		</dict>
		<key>vimVersion</key>
		<dict>
			<key>name</key>
			<string>entity.other.vim-version.vim-help</string>
			<key>match</key>
			<string>\bVim version [0-9][0-9.a-z]*</string>
		</dict>
	</dict>
	<key>fileTypes</key>
	<array>
		<string>txt</string>
	</array>
</dict>
</plist>
